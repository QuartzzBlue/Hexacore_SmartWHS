<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="300dp"
    android:layout_height="20dp"
    android:maxWidth="300dp">

    <TextView
        android:id="@+id/taskQueue_io"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="입고" />

    <TextView
        android:id="@+id/taskQueue_qty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QTY" />

    <TextView
        android:id="@+id/taskQueue_locLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="상품위치" />

    <TextView
        android:id="@+id/taskQueue_locX"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="locX" />

    <TextView
        android:id="@+id/taskQueue_locY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="locY" />
</LinearLayout>
